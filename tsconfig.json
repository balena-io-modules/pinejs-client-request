{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "node",
		"noImplicitAny": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"strictNullChecks": true,
		"sourceMap": true,
		"target": "es5"
	},
	"include": [
		"*.ts"
	]
}
