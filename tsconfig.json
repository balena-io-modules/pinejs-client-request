{
	"compilerOptions": {
		"module": "commonjs",
		"moduleResolution": "node",
		"strict": true,
		"noUnusedParameters": true,
		"noUnusedLocals": true,
		"removeComments": true,
		"preserveConstEnums": true,
		"sourceMap": true,
		"target": "es2018",
		"declaration": true,
		"skipLibCheck": true,
		"noUncheckedIndexedAccess": true
	},
	"include": [
		"*.ts"
	],
	"exclude": [
		"node_modules"
	]
}
