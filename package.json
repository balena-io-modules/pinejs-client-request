    "request": "^2.88.2",
    "typed-error": "^3.2.1"
  },
  "devDependencies": {
    "@balena/lint": "^5.1.0",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "require-npm4-to-publish": "^1.0.0",
    "typescript": "^3.9.7"
  },
  "husky": {
    "hooks": {
