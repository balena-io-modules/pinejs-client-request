  "description": "This module provides a request backend for pinejs-client.",
  "main": "request.js",
  "types": "request.d.ts",
  "repository": {
    "type": "git",
    "url": "git@github.com:resin-io-modules/pinejs-client-request.git"
  },
  "author": "Pagan Gazzard <page@resin.io>",
  "scripts": {
